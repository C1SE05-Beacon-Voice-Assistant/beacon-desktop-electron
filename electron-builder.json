{
  "appId": "com.beaconteam.autoupdater",
  "productName": "Beacon",
  "directories": {
    "output": "release/${version}"
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": ["x64"]
      }
    ],
    "artifactName": "${productName}-Windows-${version}-Setup.${ext}"
  },
  "nsis": {
    "oneClick": false,
    "perMachine": true,
    "allowToChangeInstallationDirectory": true
  },
  "publish": {
    "provider": "github",
    "releaseType": "release",
    "owner": "yensubldg",
    "repo": "C1SE05-Beacon-Voice-Assistant/beacon-desktop-electron",
    "private": true
  }
}
